{
  "buildCommand": "cd cityviewer && npm install && npm run build",
  "outputDirectory": "cityviewer/.next",
  "framework": "nextjs",
  "installCommand": "cd cityviewer && npm install",
  "regions": ["arn1"],
  "functions": {
    "cityviewer/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
}